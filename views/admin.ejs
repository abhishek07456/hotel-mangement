        <script>
            function myFunction() {
             var input, filter, table, tr, td, i;
             input = document.getElementById("myInput");
             filter = input.value.toUpperCase();
             table = document.getElementById("myTable");
             tr = table.getElementsByTagName("tr");
             for (i = 0; i < tr.length; i++) {
               td = tr[i].getElementsByTagName("td")[1];
               if (td) {
                 if (td.innerHTML.toUpperCase().startsWith(filter) > 0) {
                   tr[i].style.display = "";
                 } else {
                   tr[i].style.display = "none";
                 }
               }       
             }
           }
           </script>
      
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">user Data<i class="ti-filter"></i></h4><br>
                                
                                <input type="text" id="myInput" class="form-control border-input" onkeyup="myFunction()" placeholder="Search by organisation.." title="Type in a name">
                           
                            <div class="content table-responsive table-full-width">
                                <table class="table table-striped" id="myTable">
                                    <thead>
                                        <th>No of visitor</th>
                                        <th>organisation</th>
                                        
                                    	<th>Schedule</th>
                                        <th>Delete</th>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i < data.length; i++) { %>
                                       <tr>
                                
                                       
                                         <td><%= data[i].noofvisitors%></td>
                                         <td><%= data[i].organisation%></td>
                                         
                                         <td><a href="/bringo/record?email=<%= data[i].email%>">click</a></td>
                                         <td><a href="/bringo/record/delete?id=<%=data[i]._id%>">delete</a></td>
                                         
                                
                                       </tr>
                                    <% } %>
                                      </tbody>
                                
                                    </table>
                                

