<html>

<head>
	<meta charset="utf-8" />
	
	
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>Bringo</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link href="stylesheets/bootstrap.min.css" rel="stylesheet" />


    <link href="stylesheets/paper-dashboard.css" rel="stylesheet"/>

    
    <link href="stylesheets/themify-icons.css" rel="stylesheet">

</head>
<body>
    
<div class="wrapper">
	<div class="sidebar" data-background-color="white" data-active-color="danger">

   

    	<div class="sidebar-wrapper">
            <div class="logo">
             <center> <h3>BrinGo</h3></center>
                
            </div>

            <ul class="nav">
               
                <li class="active">
                        <a href="/bringo/login">
                            <i class="ti-panel"></i>
                            <p>login</p>
                        </a>
                    </li>
                    

            </ul>
    	</div>
    </div>

    <div class="main-panel">
		<nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar bar1"></span>
                        <span class="icon-bar bar2"></span>
                        <span class="icon-bar bar3"></span>
                    </button>
                    <a class="navbar-brand" href="#">Login</a>
                </div>
                <div class="collapse navbar-collapse">
                   
                </div>
            </div>
        </nav>


       <form action="/bringo/Auth" method="post">
        <div class="col-lg-11 col-md-10">
            <div class="card">
                <div class="header">
                        <h4 class="title"> <i class="ti-view-list-alt"></i><font color="orange"> Login</font></h4><br>
                </div>
                <div class="content">
                       
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                        <label>usertype</label>
                                        <select name="usertype"   class="form-control border-input" id="pickup" onchange="changeState()" > 
                                            <option value="transport admin">transport admin</option>
                                            <option value="admin">admin</option>
                                            <option value="faculty">faculty</option>
                                       </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                        <label>Email</label>
                                        <input type="email" name="email"  placeholder="email"  class="form-control border-input" > 

                                </div>
                            </div>
                            
                            <div class="col-md-4">
                                <div class="form-group">
                                        <label>password</label>
                                        <input type="password" name="password"  placeholder="password"  class="form-control border-input" > 

                                </div>
                            </div>

                             <br>
                             <center>
                            <input type="submit" class="btn btn-info btn-fill btn-wd"   value="login"  >
                            </center>
                         
  
 
                
                
                        </div>
                    </div>
            </div>
        </div>
      </form>
          
           

</body>

    <script src="javascripts/jquery.min.js" type="text/javascript"></script>
	<script src="javascripts/bootstrap.min.js" type="text/javascript"></script>

	
	<script src="javascripts/bootstrap-checkbox-radio.js"></script>

	<script src="javascripts/chartist.min.js"></script>

    <script src="javascripts/bootstrap-notify.js"></script>

  
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

	<script src="javascripts/paper-dashboard.js"></script>

	
	<script src="javascripts/demo.js"></script>


</html>
