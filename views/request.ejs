            <form action="/bringo/request" method="post">
                <!-------------------------------------------------------------------DIV TAG WITH ID 01 ---------------------------------------------------------------------------------->
    <div id='id01' >
            <div class="col-lg-11 col-md-10">
                    <div class="card">
                        <div class="header">
                                <h4 class="title"><font color="orange">Memeber Details</font> <i class="ti-user"></i></h4><br>
                        </div>
                        <div class="content">
                               
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Date</label>
                                    <input type="date" name="requesterdate" class="form-control border-input" required>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Departement</label>
                                                <input type="text"  name="department" placeholder="Department" class="form-control border-input" aria-required="">
                                        </div>
                                    </div>
                                </div>
    
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Name *</label>

                                     <input type="text"  name="requestername" placeholder="Name" class="form-control border-input" required>

                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                            <div class="form-group">
                                                    <label>Email *</label>
                        <input type="email"  name="requesteremail" placeholder="[a-z0-9A-z]@chikara.edu.in" pattern="[a-z0-9._%+-]+@chitkara.edu.in$" class="form-control border-input" required>
                                            </div>
                                        </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                                <label>title *</label>
                                                <input type="text"  name="title" placeholder="title" class="form-control border-input" >
                                                
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                            <div class="form-group">
                                                    <label>phone No *</label>

                                     <input type="text"  name="contact" pattern="[0-9]{10,10}"" placeholder="phone no" class="form-control border-input" required>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                                <div class="form-group">
                                                        <label>Alt phone No *</label>

                                     <input type="text"  name="altcontact"  pattern="[0-9]{10,10}" placeholder="Alt phone no" class="form-control border-input" >
                                               <br><br> </div>
                                            </div>
                                        </div>
                                    </div>
                        
                        
                                </div>
                            </div>
                        </div>
    

<!---------------------------------------------------------------------------------------DIV TAG WITH ID 02----------------------------------------------------------------------------------------------------------------->
<div id='id02' >
        <div class="col-lg-11 col-md-10">
                <div class="card">
                    <div class="header">
                            <h4 class="title"><font color="orange">visitors Details</font> <i class="ti-user"></i></h4><br>
                    </div>
                    <div class="content">
                           
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                            <label>No of Visitors</label>
                <input type="text" id="NoOfVisitors" name="noofvisitors" onchange="addFields(this.value)" class="form-control border-input" required>
          <!---->       </div>
      
                              </div>
                              <div class="col-md-4">
                                    <div id="containers" class="form-group"></div>
                              </div>
                              
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Arrival date </label>
                                            <input type="date"   name="arrivaldate" placeholder="date"  class="form-control border-input" required>
      
                                     </div>
                                    </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Arrival time</label>
                                <input type="time"   name="arrivaltime" placeholder="date"  class="form-control border-input" required>
      
                              </div>
                            </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                          
                                            <label>Deaparture Date :</label>
                                            <input type="date"  name="departuredate" placeholder="deaparture date" class="form-control border-input" >
      
                              </div>
                            </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Deaparture time :</label>
                 <input type="time"  name="departuretime" placeholder="deaparture time" class="form-control border-input" ><br>
      
                              </div>
                            </div>
</div>
</div>
</div>
</div>
