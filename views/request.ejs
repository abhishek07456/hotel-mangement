            <form action="/bringo/request" method="post">
                <!-------------------------------------------------------------------DIV TAG WITH ID 01 ---------------------------------------------------------------------------------->
    <div id='id01' >
            <div class="col-lg-11 col-md-10">
                    <div class="card">
                        <div class="header">
                                <h4 class="title"><font color="orange">Memeber Details</font> <i class="ti-user"></i></h4><br>
                        </div>
                        <div class="content">
                               
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Date</label>
                                    <input type="date" name="requesterdate" min="fun()"  class="form-control border-input" required>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Departement</label>
                                                <input type="text"  name="department" placeholder="Department" class="form-control border-input" aria-required="">
                                        </div>
                                    </div>
                                </div>
    
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                                <label>Name *</label>

                                     <input type="text"  name="requestername" placeholder="Name" class="form-control border-input" required>

                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                            <div class="form-group">
                                                    <label>Email *</label>
                        <input type="email"  name="requesteremail" placeholder="[a-z0-9A-z]@chikara.edu.in" pattern="[a-z0-9._%+-]+@chitkara.edu.in$" class="form-control border-input" required>
                                            </div>
                                        </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                                <label>title *</label>
                                                <input type="text"  name="title" placeholder="title" class="form-control border-input" required>
                                                
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                            <div class="form-group">
                                                    <label>phone No *</label>

                                     <input type="text"  name="contact" pattern="[0-9]{10,10}" placeholder="phone no" class="form-control border-input" required>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                                <div class="form-group">
                                                        <label>Alt phone No *</label>

                                     <input type="text"  name="altcontact"  pattern="[0-9]{10,10}" placeholder="Alt phone no" class="form-control border-input" >
                                               <br><br> </div>
                                            </div>
                                        </div>
                                    </div>
                        
                        
                                </div>
                            </div>
                        </div>
    

<!---------------------------------------------------------------------------------------DIV TAG WITH ID 02----------------------------------------------------------------------------------------------------------------->

<div id='id02' style="display: none">
        <div class="col-lg-11 col-md-10">
                <div class="card">
                    <div class="header">
                            <h4 class="title"><font color="orange">visitors Details</font> <i class="ti-user"></i></h4><br>
                    </div>
                    <div class="content">
                           
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                            <label>No of Visitors</label>
                <input type="text" id="NoOfVisitors" name="noofvisitors" onchange="addFields(this.value)" class="form-control border-input" required>
          <!---->       </div>
      
                              </div>
                              <div class="col-md-6">
                                    <div class="form-group">
                                            <label>Company Name</label>
                <input type="text"  name="companyName" class="form-control border-input" required>
          <!---->       </div>
      
                              </div>
                              <div class="col-md-4" id="containers">
                                   
                                    
                                    
                              </div>
                              
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Arrival date </label>
                                            <input type="Date"    name="arrivaldate" placeholder="date"  class="form-control border-input" required>
      
                                     </div>
                                    </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Arrival time</label>
                                <input type="time"   name="arrivaltime" placeholder="date"  class="form-control border-input" required>
      
                              </div>
                            </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                          
                                            <label>Deaparture Date :</label>
                                            <input type="date"  id="userdate" onchange="TDate()"  name="departuredate" placeholder="deaparture date" class="form-control border-input" required>
      
                              </div>
                            </div>
                              <div class="col-md-4">
                                    <div class="form-group">
                                            <label>Deaparture time :</label>
                 <input type="time"  name="departuretime" placeholder="deaparture time" class="form-control border-input" required><br>
      
                              </div>
                            </div>
</div>
</div>
</div>
</div>
</div>
<!-------------------------------------------------DIV TAG WITH ID 03-------------------------------------------------------------------------------->
      
        
<div id='id03' style="display: none">
        <div class="col-lg-11 col-md-6">
                  <div class="card">
                      <div class="header">
                              <h4 class="title"><font color="orange">pick up details</font> <i class="ti-exchange-vertical"></i></h4><br>
                      </div>
                      <div class="content">
                              
                              <div class="row">
                                  
                            
                                  <div class="col-md-4">
                                      <div class="form-group">
                                              <label>pickup Required :</label>
                                              <select name="pickupreq"   class="form-control border-input" id="pickup" onchange="changeState()" > 
                                              <option value="Yes">Yes</option>
                                              <option value="No">No</option>
                                   </select>
        
                                </div>
                              </div>
                              <img src="image/car.jpg">      
             <div id="idpickup" >
                                  <div class="col-md-4">
                                      <div class="form-group">
                                              <label>pickup location :</label>
                                  <input type="text" name="pickupinfo"  placeholder="location"  class="form-control border-input" > 
                                           </div></div>
                </div>
           </div>
       </div>
          
          
</div>
</div><!--end----------------------->

<div class="col-lg-11 col-md-6">
      <div class="card">
          <div class="header">
                  <h4 class="title"><font color="orange">Drop details</font> <i class="ti-exchange-vertical"></i></h4><br>
          </div>
          <div class="content">
                  
                  <div class="row">
                      
                
                      <div class="col-md-4">
                          <div class="form-group">
                                  <label>Drop Required :</label>
                                  <select name="dropreq"   class="form-control border-input" id="dropup" onchange="changeState1()" > 
                                  <option value="Yes">Yes</option>
                                  <option value="No">No</option>
                       </select>

                    </div>
                  </div>
                  <img src="image/car.jpg">      
 <div id="iddrop" >
                      <div class="col-md-4">
                          <div class="form-group">
                                  <label>Drop location :</label>
                      <input type="text" name="dropinfo"  placeholder="location"  class="form-control border-input" > 
                               </div></div>
    </div>
</div>
</div>
      </div></div>


                  <!---->  <div class="col-lg-11 col-md-9">
                        <div class="card">
                                <div class="header">
                                        <h4 class="title"><font color="orange">Meals Details</font> <i class="ti-angle-double-down"></i></h4><br>
                                </div>
                                <div class="content">
                                        
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                        <label>meal Required</label>
        
                      <select name="meals"  class="form-control border-input"> 
                    <option value="Yes">Yes</option>
                    <option value="No">No</option>
                    <option value="if later">if later</option>
                          </select>
                                                </div>
                                            </div>
                                           
        
                                           
                                            
                                            
                                </div>
                            </div>
                        </div>
            
            
                    </div><!---->
  </div>

       
      
  <input type="submit" class="btn btn-info btn-fill btn-wd"   value="submit"  >

            </div>
 </div>
    </div>

                          
             </form>

        
             

