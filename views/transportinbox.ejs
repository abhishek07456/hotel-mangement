<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	

	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>Bringo</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


  
    <link href="./stylesheets/bootstrap.min.css" rel="stylesheet" />

   
    <link href="./stylesheets/paper-dashboard.css" rel="stylesheet"/>

    <link href="./stylesheets/themify-icons.css" rel="stylesheet">

</head>

<body>
        <div class="wrapper">
                <div class="sidebar" data-background-color="white" data-active-color="danger">
            
               
            
                    <div class="sidebar-wrapper">
                        <div class="logo">
                         <center> <h3>BrinGo</h3></center>
                            
                        </div>
            
                        <ul class="nav">
                         
                            
                    <li>
                        <a href="/bringo/transportdetailsadmin">
                            <i class="ti-panel"></i>
                            <p>profile</p>
                        </a>
                    </li>
                    <li class="active">
                        <a href="/bringo/transportinbox">
                            <i class="ti-panel"></i>
                            <p>inbox</p>
                        </a>
                    </li>
                    <li>
                        <a href="/bringo/addWorker">
                            <i class="ti-panel"></i>
                            <p>Add Driver</p>
                        </a>
                    </li>
                        </ul>
                    </div>
                </div>
        
            <div class="main-panel">
                <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar bar1"></span>
                                <span class="icon-bar bar2"></span>
                                <span class="icon-bar bar3"></span>
                            </button>
                            <a class="navbar-brand" href="#">Requestor</a>
                        </div>
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav navbar-right">
                                <li>
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="ti-panel"></i>
                                        <p>Stats</p>
                                    </a>
                                </li>
                                <li class="dropdown">
                                      <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                            <i class="ti-bell"></i>
                                            <p class="notification">5</p>
                                            <p>Notifications</p>
                                            <b class="caret"></b>
                                      </a>
                                      <ul class="dropdown-menu">
                                        <li><a href="#">Notification 1</a></li>
                                        <li><a href="#">Notification 2</a></li>
                                        <li><a href="#">Notification 3</a></li>
                                        <li><a href="#">Notification 4</a></li>
                                        <li><a href="#">Another notification</a></li>
                                      </ul>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="ti-settings"></i>
                                        <p>Settings</p>
                                    </a>
                                </li>
                            </ul>
        
                        </div>
                    </div>
                </nav>
              
                <div class="content">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="card">
                                    <div class="header">
                                        <h4 class="title"><font color="orange">inbox  </font><i class="ti-filter"></i></h4><br>
                                        <input type="text" id="myInput" class="form-control border-input" onkeyup="myFunction()" placeholder="Search by name.." title="Type in a name">
                                      
                                   
                                        <div class="content table-responsive table-full-width">
                                               <table class="table table-striped" id="myTable">
                                               <thead>
                                                <th>Admin</th>
                                                <th>No of vistiors</th>
                                                <th>More details</th>
                                                 <th>click</th>
                                                <th>status</th>
                                               
                                            </thead>
                                            
                                                <tbody>
                                                    <tr>
                                                     
                                                    <% for(var i=0; i < data.length; i++) { %>
                                                     <td>Sukhmit</td>
                                                    <td><%= data[i].noofvisitors%></td>
                                                     <td><a href="/bringo/record?id=<%= data[i]._id%>">check</a> </td>
                                                    
                                                     <td> <a href="/bringo/transport_details?id=<%=data[i]._id%>">click</a></td>
                                                     
                                                     <td><font color="green"><%= data[i].status%></font></td>
                                         
                                            
                                                   </tr>
                                                    
                                                <% } %>
                                               
                                        
                                            </table>
                                             
        
                                            </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        
                           
          
   
        
        <!----footer-------------------------------------->
        <footer class="footer">
            <div class="container-fluid">
            
            </div>
        </footer>
    
 
<!----footer-------------------------------------->
</body>
<script src="request.js"></script>
<script src="javascripts/serachElement.js"></script> 





    <script src="javascripts/jquery.min.js" type="text/javascript"></script>
	<script src="javascripts/bootstrap.min.js" type="text/javascript"></script>

	<!--  Checkbox, Radio & Switch Plugins -->
	<script src="javascripts/bootstrap-checkbox-radio.js"></script>

	<!--  Charts Plugin -->
	<script src="javascripts/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="javascripts/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

    <!-- Paper Dashboard Core javascript and methods for Demo purpose -->
	<script src="javascripts/paper-dashboard.js"></script>

	<!-- Paper Dashboard DEMO methods, don't include it in your project! -->
	<script src="javascripts/demo.js"></script>


</html>
