						<li>
                            <a href="#">
								<i class="ti-settings"></i>
								<p>Settings</p>
                            </a>
                        </li>
                    </ul>

                </div>
            </div>
        </nav>
        <div class="col-lg-11 col-md-9">
            <div class="card">
                <div class="header">
                        <h4 class="title"><font color="orange"> Admin Details</font> <i class="ti-user"></i></h4><br>
                </div>
                <div class="content">
                        <form action="/bringo/transport-admin-details" method="post">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                        <label>User-Name</label>
                                        <input type="text"  name="TadminUserName"  placeholder="User Name" class="form-control border-input" required>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                        <label>Email id</label>
                                        <input type="email"   name="TadminEmail" placeholder="email"  class="form-control border-input">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                        <label>Name</label>
                                        <input type="text"  name="TadminName" id="dep" placeholder="Name"  class="form-control border-input" required>
                                </div>
                            </div>
                      

                       
                            <div class="col-md-4">
                                <div class="form-group">
                                        <label>contact</label>
                                        <input type="text"  placeholder="Phoneno" name="TadminPhoneno"  pattern="[6789][0-9]{8}" class="form-control border-input" required>
                                </div>
                            </div>
                            
             
                           
                             
                                <div class="col-md-4">
                                        <div class="form-group">
                                                <label>Update password</label>
                                                <input type="password"   name="TadminPassword" placeholder="new password" class="form-control border-input">
                                        </div>
                                    </div>
                              
                                
                        </div>

                        <input type="submit" class="btn btn-info btn-fill btn-wd"   value="change details"  >
                    </form>
                </div>
            </div>
        </div>


    </div>
</div>
