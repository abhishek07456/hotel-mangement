<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	

	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>Bringo</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


  
    <link href="./stylesheets/bootstrap.min.css" rel="stylesheet" />

   
    <link href="./stylesheets/paper-dashboard.css" rel="stylesheet"/>

    <link href="./stylesheets/themify-icons.css" rel="stylesheet">

</head>

<body>
  
    <div class="wrapper">
        <div class="sidebar" data-background-color="white" data-active-color="danger">
    
       
    
            <div class="sidebar-wrapper">
                <div class="logo">
                 <center> <h3>BrinGo</h3></center>
                    
                </div>
    
                <ul class="nav">
                 
                    <li class="active">
                        <a href="/bringo/inbox">
                            <i class="ti-panel"></i>
                            <p>inbox</p>
                        </a>
                    </li>
                   
                    <li>
                        <a href="/bringo/faculty">
                            <i class="ti-panel"></i>
                            <p>Add Faculty</p>
                        </a>
                    </li>
                    <li>
                            <a href="/bringo/addWorker">
                                <i class="ti-panel"></i>
                                <p>Add Workers</p>
                            </a>
                        </li>
                    <li >
                        <a href="/bringo/arrangement">
                            <i class="ti-panel"></i>
                            <p>visitor Arrangement</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>

    <div class="main-panel">
		<nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar bar1"></span>
                        <span class="icon-bar bar2"></span>
                        <span class="icon-bar bar3"></span>
                    </button>
                    <a class="navbar-brand" href="#">Requestor</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="ti-panel"></i>
								<p>Stats</p>
                            </a>
                        </li>
                        <li class="dropdown">
                              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="ti-bell"></i>
                                    <p class="notification">5</p>
									<p>Notifications</p>
									<b class="caret"></b>
                              </a>
                              <ul class="dropdown-menu">
                                <li><a href="#">Notification 1</a></li>
                                <li><a href="#">Notification 2</a></li>
                                <li><a href="#">Notification 3</a></li>
                                <li><a href="#">Notification 4</a></li>
                                <li><a href="#">Another notification</a></li>
                              </ul>
                        </li>
						<li>
                            <a href="#">
								<i class="ti-settings"></i>
								<p>Settings</p>
                            </a>
                        </li>
                    </ul>

                </div>
            </div>
        </nav>
      
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title"><font color="orange">Requestor Details</font><i class="ti-filter"></i></h4><br>
                                <input type="text" id="myInput" class="form-control border-input" onkeyup="myFunction()" placeholder="Search by name.." title="Type in a name">
                              
                           
                                <div class="content table-responsive table-full-width">
                                       <table class="table table-striped" id="myTable">
                                       <thead>
                                        <th>department</th>
                                        <th>name</th>
                                        <th>email</th>
                                        <th>date</th>
                                        <th>arrangement</th>
                                        <th>Delete Record</th>
                                       
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i < data.length; i++) { %>
                                       <tr>
                                
                                         <td><%= data[i].department%></td>
                                         <td><%= data[i].requestername%></td>
                                         <td><%= data[i].requesteremail%></td>
                                         <td><%= data[i].requesterdate%></td>
                                         <td><a href="/bringo/visitorAdmin?email=<%= data[i].requesteremail%>&date=<%= data[i].requesterdate%>">click</a></td>
                                         <td><a href="/bringo/request/delete?id=<%=data[i]._id%>">delete</a></td>
                                         
                                
                                       </tr>
                                    <% } %>
                                      </tbody>
                                
                                    </table>
                                

                                    </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

                   

     

<!----footer-------------------------------------->
<footer class="footer">
    <div class="container-fluid">
    
    </div>
</footer>
<!----footer-------------------------------------->
</body>
<script src="javascripts/serachElement.js"></script> 





    <script src="javascripts/jquery.min.js" type="text/javascript"></script>
	<script src="javascripts/bootstrap.min.js" type="text/javascript"></script>

	<!--  Checkbox, Radio & Switch Plugins -->
	<script src="javascripts/bootstrap-checkbox-radio.js"></script>

	<!--  Charts Plugin -->
	<script src="javascripts/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="javascripts/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

    <!-- Paper Dashboard Core javascript and methods for Demo purpose -->
	<script src="javascripts/paper-dashboard.js"></script>

	<!-- Paper Dashboard DEMO methods, don't include it in your project! -->
	<script src="javascripts/demo.js"></script>


</html>
